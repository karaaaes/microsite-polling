@media (min-width: 768px) and (max-width: 991.98px){.btn-next,.btn-prev{width:fit-content}.header{padding:12px 16px;width:calc(100% - 32px)}.header .header-row{width:100%}.body{margin-top:92px}.container,.header-row{width:100%;max-width:100%}.card-wrapper{padding-left:16px;padding-right:16px}.card-wrapper .card-row .card-fit{min-height:calc(100vh - 450px)}.card-wrapper .card-row .card-full{min-height:calc(100vh - 482px)}.card-logo{width:100%}.card-logo .logo-list{display:flex;flex-wrap:wrap;align-items:center;justify-content:center;width:343px;margin:auto}.card-logo .logo-list .logo{width:fit-content}.footer{height:48px}}
